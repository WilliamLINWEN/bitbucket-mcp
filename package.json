{
  "name": "bitbucket-mcp-server",
  "version": "1.0.4",
  "description": "MCP server for Bitbucket tools and integration",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "bitbucket-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
    "start": "npm run build && node build/index.js",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mcp",
    "bitbucket",
    "model-context-protocol"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WilliamLINWEN/bitbucket-mcp.git"
  },
  "homepage": "https://github.com/WilliamLINWEN/bitbucket-mcp",
  "author": "WilliamLin",
  "license": "ISC",
  "devDependencies": {
    "@vitest/ui": "^2.1.8",
    "happy-dom": "^15.11.7",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@types/node": "^22.15.21",
    "bitbucket-mcp-server": "^1.0.3",
    "node-fetch": "^3.3.2",
    "zod": "^3.25.28"
  }
}
